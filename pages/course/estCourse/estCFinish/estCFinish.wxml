<!--pages/estCFinish/estCFinish.wxml-->
<view class='page'>
  <view class='page-head'>
    <image class='page-hd-icon' src='../../../../image/finish.png' ></image>
    <view><text>创建成功</text></view>
  </view>
  <view class='page-body'>
    <view class='page-bd-title'>您的班级信息</view>
    <view class='course-info'>
      <view>班级号：{{}}</view>
      <view>课程名称：{{cName}}</view>
      <view>班级名称：{{cClass}}</view>
      <view>任课老师：{{tName}}</view>
    </view>
    <button  class='finish' 
      loading="{{loading}}" disabled="{{disabled}}" 
      bindtap="finish">确定
    </button>
  </view>
</view>
