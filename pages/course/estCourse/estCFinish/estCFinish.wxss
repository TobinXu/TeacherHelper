/* pages/estCFinish/estCFinish.wxss */
.page{
  background-color: #fff;
  height: 100vh;
}
.page-head{
  text-align: center;
  height: 200px;
  width: 100%;
  font-size: 20px;
  color: #353535;
}
  .page-hd-icon{
    height: 112px;
    width: 112px;
    margin-top: 50px;
  }
  .page-body{
    width: 60%;
    margin: 0 auto;
  }
    .page-bd-title{
      font-size: 14px;
      color: #959494;
      padding: 7px 0;
    }
    .course-info{
      width: 100%;
      height: 160px;
      line-height: 30px;
      border-radius: 5px;
      background-color: rgba(140, 174, 133, 1);
      text-align: center;
      box-shadow: 2px 2px 5px 0px rgba(170, 170, 170, 1);
      border: 1px solid rgba(255, 255, 255, 0);
      font-size: 14px;
      color: #fff;
      display: flex;
      justify-content: center;
      flex-direction: column; 
    }

  .finish{
    width: 50%;
    background-color: rgb(140, 174, 133);
    color: white;

    border-radius: 18px;
    height: 40px;
    font-size: 16px;
    margin-top: 60px;
  }